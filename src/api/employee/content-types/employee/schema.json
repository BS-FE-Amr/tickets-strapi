{
  "kind": "collectionType",
  "collectionName": "employees",
  "info": {
    "singularName": "employee",
    "pluralName": "employees",
    "displayName": "Employee"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": false,
      "unique": true
    },
    "username": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "password": {
      "type": "password",
      "required": false
    },
    "age": {
      "type": "integer",
      "required": true,
      "min": 18,
      "max": 60
    },
    "todos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::todo.todo",
      "mappedBy": "employee"
    }
  }
}
