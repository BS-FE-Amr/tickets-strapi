{
  "kind": "collectionType",
  "collectionName": "employees",
  "info": {
    "singularName": "employee",
    "pluralName": "employees",
    "displayName": "Employee"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "username": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "password": {
      "type": "password",
      "required": true
    },
    "age": {
      "type": "integer",
      "required": true,
      "min": 18,
      "max": 60
    }
  }
}
